email_cadastrado = "abc@gmail.com"
senha_cadastrada = "123"

email_usuario = ""
senha_usuario = ""


while email_usuario != email_cadastrado or senha_usuario != senha_cadastrada:
    #
    email_usuario = input("Digite seu email: ")
    senha_usuario = input("Digite sua senha: ")


    if email_usuario != email_cadastrado or senha_usuario != senha_cadastrada:
        print("Email ou senha incorretos. Tente novamente.")


print("Login bem-sucedido!")
